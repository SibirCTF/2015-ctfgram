<%- include('header'); -%>
    <div class="container">
      <div class="blog-header">
        <h1 class="blog-title">Nickname's photos</h1>
        <p class="lead blog-description">Some photos about me.</p>
      </div>

      <div class="row">
        <div class="col-sm-12 blog-main">
          <div class="row photos">
            <% list.forEach(function(item){ %>
              <a href="/photo/<%= item.id %>">
                <div class="col-md-3 item">
                  <img width="100%" src="<%= item.url %>">
                </div>
              </a>
            <% }) %>
          </div>
         
          <nav>
            <ul class="pager">
              <li><a href="#">Previous</a></li>
              <li><a href="#">Next</a></li>
            </ul>
          </nav>

        </div><!-- /.blog-main -->
      </div><!-- /.row -->

    </div><!-- /.container -->
<%- include('footer'); -%>